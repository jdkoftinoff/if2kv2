<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>ServiceDescription</key>
    <string>The Internet Filter IF-2K</string>
	<key>Label</key>
	<string>com.internetfilter.if2kd</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/If2k/bin/if2kd</string>
		<string>--log.detail=1</string>
	  <string>--log.file=/Library/If2k/log.txt</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/Library/If2k/err.txt</string>
	<key>StandardOutPath</key>
	<string>/Library/If2k/out.txt</string>
  <key>OnDemand</key>
  <false/>
</dict>
</plist>
